{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["ES2019", "ES2019.Array"],
    "module": "ESNext",
    "outDir": "lib",
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "ESNext",
    "types": ["node", "jest"],
  },
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": false
  }
}
